{
  "name": "server-rocketcher-customize",
  "packageManager": "yarn@3.2.3",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "----- Web Server -----": "",
    "web-start": "cd packages/web-server && react-scripts start",
    "web-build": "cd packages/web-server && react-scripts build",
    "----- Back Server -----": "",
    "back-start": "cd packages/back-server && nodemon --exec ts-node src/index.ts",
    "----- All Server -----": "",
    "all-start": "yarn back-start & yarn web-start"
  },
  "devDependencies": {
    "express": "^4.18.1",
    "nodemon": "^2.0.19",
    "react-scripts": "5.0.1",
    "sass": "^1.53.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.2"
  }
}
